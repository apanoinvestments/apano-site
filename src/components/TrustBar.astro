---
const { items } = Astro.props;
---
<section style="border-top:1px solid var(--line);border-bottom:1px solid var(--line);">
  <div class="container" style="display:flex;gap:10px;flex-wrap:wrap;padding:14px var(--pad);">
    {(items ?? []).map((it) => (
      <span class="small" style="border:1px solid var(--line);border-radius:999px;padding:8px 10px;">
        {it.label}
      </span>
    ))}
  </div>
</section>
